# VulnExplain Frontend Environment Variables
# Copy this to .env.local and fill in actual values

# Backend API URL
VITE_API_URL=http://localhost:5000

# Optional: Gemini API Key (only if making direct calls from frontend - NOT RECOMMENDED)
# VITE_GEMINI_API_KEY=your_key_here

# Feature flags
VITE_ENABLE_REPO_AUDIT=true
VITE_ENABLE_ANALYTICS=false
VITE_DEBUG_MODE=false